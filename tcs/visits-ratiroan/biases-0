proposal::setidentifier "2012A-0006"
visit::setidentifier "0"
visit::setname "biases"
block::settotalexposures 0
visit::settargetcoordinates fixed 0h 30d

proc SELECTABLE {args} {
  return [expr {
    [withintelescopepointinglimits] && 
    [maxskybrightness "civiltwilight"]
  }]
}

proc EXECUTE {args} {
  setsecondaryoffset 0
  move
  movefilterwheel "r"
  set i 0
  while {$i < 20} {
    exposebias
    incr i
  }
  return true
}
