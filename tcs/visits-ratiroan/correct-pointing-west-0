proposal::setidentifier "2017A-0006";
visit::setidentifier "1"
visit::setname "pointing correction in west"
block::settotalexposures 0
visit::settargetcoordinates fixed +1h +30d 2000

proc SELECTABLE {args} {
  return [expr {
    [maxfocusdelay 1800] &&
    [withintelescopepointinglimits] && 
    [maxskybrightness "nauticaltwilight"] &&
    [minmoonseparation "15d"] 
  }]
}

proc EXECUTE {args} {
  setsecondaryoffset 0
  setbinning 1
  setreadmode 1MHz
  tracktopocentric
  movefilterwheel "W"
  correctpointing C0 20
  return true
}
