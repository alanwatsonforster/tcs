proposal::setidentifier "[utcclock::semester]-0006";
visit::setidentifier "0"
visit::setname "pointing correction in east"
block::settotalexposures 0
visit::settargetcoordinates fixed -1h +30d 2000

proc SELECTABLE {args} {
  return [expr {
    [maxfocusdelay 1800] &&
    [withintelescopepointinglimits] && 
    [maxskybrightness "nauticaltwilight"] &&
    [minmoonseparation "15d"] 
  }]
}

proc EXECUTE {args} {

  setsecondaryoffset 0
  tracktopocentric

  executor::setreadmode 1MHz
  executor::setwindow "default"
  executor::setbinning 2
  movefilterwheel "w"

  waituntiltracking

  correctpointing C0 20

  return true
}
