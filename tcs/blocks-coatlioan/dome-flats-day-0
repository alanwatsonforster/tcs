proposal::setidentifier "[utcclock::semester]-0006"
visit::setidentifier "0"
visit::setname "dome flats"
block::settotalexposures 0
visit::settargetcoordinates zenith

proc SELECTABLE {args} {
  return [expr {
    [withintelescopepointinglimits]
  }]
}

proc EXECUTE {args} {

  setsecondaryoffset 0
  move

  setwindow "default"
  movefilterwheel "BB"

  foreach readmode {1MHz 6MHz} {
    setreadmode $readmode
    foreach binning {1 2} {
      setbinning $binning
      set i 0
      while {$i < 4} {
        expose flat [expr {1.0 / $binning / $binning}]
        analyze levels
        incr i
      }
    }
  }

  return false
}