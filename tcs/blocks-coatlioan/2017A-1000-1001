proposal::setidentifier "2017A-1000"
block::setidentifier "1001"
visit::setidentifier "0"
visit::setname "SDSS J172421.74+654847.5"
block::settotalexposures 0
visit::settargetcoordinates equatorial 17:24:21.72 +65:48:47.3 2000

proc SELECTABLE {args} {
  return [expr {
    [withintelescopepointinglimits] && 
    [maxskybrightness "nauticaltwilight"] &&
    [maxairmass 2.0] &&
    [minmoonseparation "15d"] &&
    [maxfocusdelay 1500] &&
    ([maxha "-0.5h"] || [minha "+0h"])
  }]
}

proc EXECUTE {args} {

  set exposuretime       30

  set exposuresperdither 2
  set filters            {BR}
#  set exposuresperdither 1
#  set filters {BV BR BR BV}
  
  executor::setsecondaryoffset 0
  executor::track

  executor::setreadmode 1MHz
  executor::setwindow "default"
  executor::setbinning 2

  executor::waituntiltracking

  foreach {eastoffset northoffset} {
      0as   0as
    +30as +30as
    +30as -30as
    -30as -30as
    -30as +30as
      0as -30as
    +30as   0as
      0as +30as
    -30as   0as
  } {
    executor::offset $eastoffset $northoffset "default"
    executor::waituntiltracking
    foreach filter $filters {
      executor::movefilterwheel $filter
      set i 0
      while {$i < $exposuresperdither} {
        executor::expose object $exposuretime
        incr i
      }
    }
  }

  return false
}
