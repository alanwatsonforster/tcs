N=25

paste \
  <(jot -r -w "%c" $(expr $N "*" 2) A Z | rs -g 0 2) \
  <(jot -r -w "%c" $(expr $N "*" 4) a z | rs -g 0 4) \
  <(jot -r -w "%d" $(expr $N "*" 4) 0 9 | rs -g 0 4) \
| expand | sed 's/  *//g' |
sed 's/\(.\)\(.\)\(.\)\(.\)\(.\)\(.\)\(.\)\(.\)/\1\3\4\7\8\2\5\6/'



